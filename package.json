{
    "name": "projeto",
    "version": "1.0.0",
    "scripts": {
        "build": "node ace build --production",
        "start": "node server.js",
        "dev": "node ace serve --watch",
        "lint": "eslint . --ext=.ts",
        "format": "prettier --write .",
        "db:up": "docker volume create --name=postgresdata && docker compose up -d",
