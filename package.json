        "@adonisjs/repl": "^3.1.10",
        "nyc": "^15.1.0",
        "pg": "^8.7.3",
        "phc-argon2": "^1.1.3",
        "phc-bcrypt": "^1.0.7",
        "proxy-addr": "^2.0.7",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.21",
        "yarn": "^1.22.18"
    },
    "devDependencies": {
        "@adonisjs/assembler": "^5.6.2",
        "@types/source-map-support": "^0.5.4",
        "adonis-preset-ts": "^2.1.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
