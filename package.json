{
    "name": "projeto",
    "version": "1.0.0",
    "scripts": {
        "build": "node ace build --production",
        "start": "node server.js",
        "dev": "node ace serve --watch",
        "lint": "eslint . --ext=.ts",
        "format": "prettier --write .",
        "db:up": "docker volume create --name=postgresdata && docker compose up -d",
        "prepare:db": "node ace migration:run && node ace db:seed",
        "prepare:migration": "node ace migration:run",
        "test": "node -r @adonisjs/assembler/build/register japaFile.ts",
        "test:ci": "nyc --reporter=cobertura --reporter=html node -r @adonisjs/assembler/build/register japaFile.ts"
    },
    "dependencies": {
        "@adonisjs/auth": "^8.0.6",
        "@adonisjs/core": "^5.1.0",
        "@adonisjs/lucid": "^10.0.0",
        "@adonisjs/repl": "^3.0.0",
        "axios": "^0.21.1",
        "nyc": "^15.1.0",
        "pg": "^8.6.0",
        "phc-argon2": "^1.1.1",
        "phc-bcrypt": "^1.0.7",
        "proxy-addr": "^2.0.7",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.19"
    },
    "devDependencies": {
        "@adonisjs/assembler": "^5.0.0",
        "@types/jsdom": "^16.2.11",
        "@types/supertest": "^2.0.11",
        "adonis-preset-ts": "^2.1.0",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-adonis": "^1.3.2",
        "eslint-plugin-prettier": "^3.4.0",
        "execa": "^5.1.1",
        "get-port": "^5.1.1",
        "japa": "^3.1.1",
        "jsdom": "^16.6.0",
        "pino-pretty": "^5.0.2",
        "prettier": "^2.3.1",
        "supertest": "^6.1.3",
        "typescript": "~4.2",
        "youch": "^2.2.2",
        "youch-terminal": "^1.1.1"
    }
}